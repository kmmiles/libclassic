    "blessed-contrib": "^4.8.20",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "csvtojson": "^2.0.10",
    "husky": "^4.2.5",
    "ts-node": "8.5.4",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "js-base64": "^2.5.2",
    "json-query": "^2.2.2",
    "lodash.clonedeep": "^4.5.0",
    "mathjs": "^7.0.1",
    "pako": "^1.0.11",
    "statsjs": "^1.0.7"
  }
