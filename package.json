    "src",
    "assets",
    "whitepaper",
    "docs"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "cli:gear": "ts-node -sT cli/gear.ts",
    "cli:playground": "ts-node -sT cli/playground.ts",
    "lint": "tsdx lint",
    "test": "tsdx test",
    "build": "tsdx --format cjs,esm,umd build",
    "items": "ts-node -sT cli/convert-items.ts > src/wow/db/items.json",
    "docs": "typedoc --out docs src",
    "prepare": "tsdx --format cjs,esm,umd build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "name": "libclassic",
  "author": "kmmiles",
  "module": "dist/libclassic.esm.js",
  "devDependencies": {
    "@types/blessed": "^0.1.17",
    "@types/file-saver": "^2.0.1",
